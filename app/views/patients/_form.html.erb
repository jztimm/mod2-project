<%= form_for @patient do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :password %>
  <%= f.password_field :password %>
  
  <%= f.label :age %>
  <%= f.number_field :age, min: 0, max: 150%>
  
  <%= f.label :city %>
  <%= f.text_field :city %>
  
  <%= f.label :zipcode %>
  <%= f.number_field :zipcode %>

  <%= f.submit %>
<% end %>